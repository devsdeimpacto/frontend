    "build": "webpack --mode production --env REACT_APP_ENV=production",
    "start": "webpack serve --mode development --env REACT_APP_ENV=local",
    "build:develop": "webpack --mode production --env REACT_APP_ENV=development",
    "build:beta": "webpack --mode production --env REACT_APP_ENV=beta",
    "build:stage": "webpack --mode production --env REACT_APP_ENV=staging",
    "build:prod": "webpack --mode production --env REACT_APP_ENV=production",
    "postinstall": "npx husky install",
    "lint": "eslint ./resources --ext .js,.jsx",
    "test": "jest --verbose --bail --no-cache",
    "docs": "jsdoc -c ./jsdocs.json -r -d ./.docs",
    "stats": "mkdir -p .stats && webpack --profile --json > .stats/stats.json",
    "analyzer": "webpack-bundle-analyzer .stats/stats.json ./public/assets/js",
    "railway": "serve -s public -l $PORT"
